{
    "name": "laravel/framework",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "homepage": "https://laravel.com",
    "support": {
        "issues": "https://github.com/laravel/framework/issues",
        "source": "https://github.com/laravel/framework"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "doctrine/inflector": "^1.1",
        "dragonmantank/cron-expression": "^1.2.1",
        "egulias/email-validator": "^2.1.10",
        "ircmaxell/password-compat": "~1.0",
        "league/commonmark": "^0.18.5",
        "league/commonmark-ext-table": "^0.9.0",
        "monolog/monolog": "^1.25",
        "moontoast/math": "^1.1",
        "opis/closure": "^3.6",
        "paragonie/random_compat": "~1.4|~2.0|~9.99",
        "psr/container": "^1.0",
        "psr/log": "^1.0",
        "psr/simple-cache": "^1.0",
        "ramsey/uuid": "^3.7",
        "swiftmailer/swiftmailer": "^5.4",
        "symfony/console": "^3.4",
        "symfony/finder": "^3.4",
        "symfony/http-foundation": "^3.4",
        "symfony/http-kernel": "^3.4",
        "symfony/intl": "~2.3|~3.0|~4.0|~5.0",
        "symfony/process": "^3.4",
        "symfony/translation": "^3.4",
        "symfony/var-dumper": "^3.4",
        "tijsverkoyen/css-to-inline-styles": "^2.2.2",
        "vlucas/phpdotenv": "^4.0",
        "voku/portable-utf8": "^3.1.30"
    },
    "replace": {
        "illuminate/auth": "self.version",
        "illuminate/broadcasting": "self.version",
        "illuminate/bus": "self.version",
        "illuminate/cache": "self.version",
        "illuminate/collections": "self.version",
        "illuminate/config": "self.version",
        "illuminate/console": "self.version",
        "illuminate/container": "self.version",
        "illuminate/contracts": "self.version",
        "illuminate/cookie": "self.version",
        "illuminate/database": "self.version",
        "illuminate/encryption": "self.version",
        "illuminate/events": "self.version",
        "illuminate/filesystem": "self.version",
        "illuminate/hashing": "self.version",
        "illuminate/http": "self.version",
        "illuminate/log": "self.version",
        "illuminate/macroable": "self.version",
        "illuminate/mail": "self.version",
        "illuminate/notifications": "self.version",
        "illuminate/pagination": "self.version",
        "illuminate/pipeline": "self.version",
        "illuminate/queue": "self.version",
        "illuminate/redis": "self.version",
        "illuminate/routing": "self.version",
        "illuminate/session": "self.version",
        "illuminate/support": "self.version",
        "illuminate/testing": "self.version",
        "illuminate/translation": "self.version",
        "illuminate/validation": "self.version",
        "illuminate/view": "self.version"
    },
    "require-dev": {
        "aws/aws-sdk-php": "^3.155",
        "doctrine/dbal": "^2.5",
        "fakerphp/faker": "^1.9.2",
        "guzzlehttp/guzzle": "^6.5.5",
        "league/flysystem-cached-adapter": "^1.0",
        "mockery/mockery": "^1.3.1",
        "pda/pheanstalk": "^3.0",
        "phpunit/phpunit": "^5.7",
        "predis/predis": "^1.1.1",
        "symfony/cache": "^3.4"
    },
    "provide": {
        "psr/container-implementation": "1.0"
    },
    "conflict": {
        "tightenco/collect": "<5.5.33"
    },
    "bin": [
        "cr/patch/carbon/bin/carbon"
    ],
    "autoload": {
        "exclude-from-classmap": [
            "vendor/symfony/http-kernel/Exception"
        ],
        "files": [
            "cr/helpers/helpers.php",
            "cr/helpers/backport.php",
            "cr/helpers/casts.php",
            "cr/patch/symfony/polyfill/src/bootstrap.php",
            "cr/patch/symfony/polyfill/src/Apcu/bootstrap.php",
            "cr/patch/symfony/polyfill/src/Ctype/bootstrap.php",
            "cr/patch/symfony/polyfill/src/Uuid/bootstrap.php",
            "cr/patch/symfony/polyfill/src/Iconv/bootstrap.php",
            "cr/patch/symfony/polyfill/src/Intl/Grapheme/bootstrap.php",
            "cr/patch/symfony/polyfill/src/Intl/Idn/bootstrap.php",
            "cr/patch/symfony/polyfill/src/Intl/Icu/bootstrap.php",
            "cr/patch/symfony/polyfill/src/Intl/MessageFormatter/bootstrap.php",
            "cr/patch/symfony/polyfill/src/Intl/Normalizer/bootstrap.php",
            "cr/patch/symfony/polyfill/src/Mbstring/bootstrap.php",
            "cr/symfony/deprecation-contracts/function.php",
            "src/Illuminate/Collections/helpers.php",
            "src/Illuminate/Events/functions.php",
            "src/Illuminate/Foundation/helpers.php",
            "src/Illuminate/Support/helpers.php"
        ],
        "psr-4": {
            "Symfony\\Polyfill\\": "cr/patch/symfony/polyfill/src/",
            "Symfony\\Component\\ErrorHandler\\": "cr/symfony/error-handler/",
            "Symfony\\Component\\HttpFoundation\\": "cr/patch/symfony/http-foundation/",
            "Symfony\\Component\\HttpKernel\\Exception\\": "cr/patch/symfony/http-kernel/Exception/",
            "Symfony\\Component\\Mime\\": "cr/symfony/mime/",
            "Symfony\\Component\\Routing\\": "cr/symfony/routing/",
            "League\\Flysystem\\": "cr/patch/flysystem/src/",
            "Carbon\\": "cr/patch/carbon/src/",
            "Doctrine\\": "cr/patch/doctrine/doctrine/",
            "Illuminate\\": "src/Illuminate/",
            "Illuminate\\Support\\": ["src/Illuminate/Macroable/", "src/Illuminate/Collections/"],
            "Illuminate\\Foundation\\Auth\\": "cr/patch/laravel-ui/auth-backend/",
            "Laravel\\Ui\\": "cr/patch/laravel-ui/src/",
            "Sushi\\": "cr/patch/sushi/sushi/",
            "CR\\LaravelBackport\\": "cr/patch/laravel-backport/"
        },
        "classmap": [
            "cr/patch/symfony/polyfill/src/Intl/Icu/Resources/stubs",
            "cr/patch/symfony/polyfill/src/Intl/MessageFormatter/Resources/stubs",
            "cr/patch/symfony/polyfill/src/Intl/Normalizer/Resources/stubs",
            "cr/patch/symfony/polyfill/src/Php80/Resources/stubs",
            "cr/patch/symfony/polyfill/src/Php73/Resources/stubs"
        ]
    },
    "autoload-dev": {
        "exclude-from-classmap": [
            "vendor/phpunit/phpunit/src/ForwardCompatibility/TestCase.php"
        ],
        "files": [
            "cr/helpers/tests.php",
            "tests/Database/stubs/MigrationCreatorFakeMigration.php"
        ],
        "psr-4": {
            "Orchestra\\Testbench\\": "cr/patch/orchestra-testbench-core/testbench-core/src/",
            "PHPUnit\\": "cr/patch/phpunit-src/PHPUnit/",
            "Illuminate\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-merge": "5.4-dev"
        },
        "laravel": {
            "providers": [
                "Carbon\\Laravel\\ServiceProvider"
            ]
        }
    },
    "suggest": {
        "ext-ftp": "Required to use the Flysystem FTP driver.",
        "ext-gd": "Required to use Illuminate\\Http\\Testing\\FileFactory::image().",
        "ext-memcached": "Required to use the memcache cache driver.",
        "ext-pcntl": "Required to use all features of the queue worker.",
        "ext-posix": "Required to use all features of the queue worker.",
        "ext-redis": "Required to use the Redis cache and queue drivers (^4.0|^5.0).",
        "aws/aws-sdk-php": "Required to use the SQS queue driver, DynamoDb failed job storage and SES mail driver (^3.155).",
        "brianium/paratest": "Required to run tests in parallel (^6.0).",
        "doctrine/dbal": "Required to rename columns and drop SQLite columns (^2.12|^3.0).",
        "filp/whoops": "Required for friendly error pages in development (^2.8).",
        "fakerphp/faker": "Required to use the eloquent factory builder (^1.9.1).",
        "guzzlehttp/guzzle": "Required to use the HTTP Client, Mailgun mail driver and the ping methods on schedules (^7.2).",
        "laravel/tinker": "Required to use the tinker console command (^2.0).",
        "league/flysystem-aws-s3-v3": "Required to use the Flysystem S3 driver (^2.0).",
        "league/flysystem-ftp": "Required to use the Flysystem FTP driver (^2.0).",
        "league/flysystem-sftp": "Required to use the Flysystem SFTP driver (^2.0).",
        "mockery/mockery": "Required to use mocking (^1.4.2).",
        "nyholm/psr7": "Required to use PSR-7 bridging features (^1.2).",
        "pda/pheanstalk": "Required to use the beanstalk queue driver (^4.0).",
        "phpunit/phpunit": "Required to use assertions and run tests (^9.4).",
        "predis/predis": "Required to use the predis connector (^1.1.2).",
        "psr/http-message": "Required to allow Storage::put to accept a StreamInterface (^1.0).",
        "pusher/pusher-php-server": "Required to use the Pusher broadcast driver (^5.0|^6.0).",
        "symfony/cache": "Required to PSR-6 cache bridge (^5.3).",
        "symfony/filesystem": "Required to enable support for relative symbolic links (^5.3).",
        "symfony/psr-http-message-bridge": "Required to use PSR-7 bridging features (^2.0).",
        "wildbit/swiftmailer-postmark": "Required to use Postmark mail driver (^3.0)."
    },
    "config": {
        "platform": {
            "php": "5.6.40"
        },
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
